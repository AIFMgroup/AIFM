{
  "name": "aifm-frontend-build",
  "description": "Build and deploy AIFM Frontend to ECS",
  "source": {
    "type": "S3",
    "location": "aifm-codebuild-source/source.zip"
  },
  "artifacts": {
    "type": "NO_ARTIFACTS"
  },
  "environment": {
    "type": "LINUX_CONTAINER",
    "image": "aws/codebuild/amazonlinux2-x86_64-standard:5.0",
    "computeType": "BUILD_GENERAL1_MEDIUM",
    "privilegedMode": true,
    "environmentVariables": [
      {
        "name": "ECR_REGISTRY",
        "value": "798076332693.dkr.ecr.eu-north-1.amazonaws.com",
        "type": "PLAINTEXT"
      },
      {
        "name": "ECR_REPO",
        "value": "aifm-frontend",
        "type": "PLAINTEXT"
      },
      {
        "name": "ECS_CLUSTER",
        "value": "aifm-prod",
        "type": "PLAINTEXT"
      },
      {
        "name": "ECS_SERVICE",
        "value": "AIFM_WEB-service",
        "type": "PLAINTEXT"
      },
      {
        "name": "AWS_DEFAULT_REGION",
        "value": "eu-north-1",
        "type": "PLAINTEXT"
      }
    ]
  },
  "serviceRole": "arn:aws:iam::798076332693:role/aifm-codebuild-role",
  "timeoutInMinutes": 30,
  "queuedTimeoutInMinutes": 60,
  "logsConfig": {
    "cloudWatchLogs": {
      "status": "ENABLED",
      "groupName": "/aws/codebuild/aifm-frontend-build"
    }
  }
}
