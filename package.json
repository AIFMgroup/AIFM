{
  "name": "aifm-agent-portal",
  "version": "0.1.0",
  "description": "AIFM Agent Portal - AWS Native",
  "private": true,
  "workspaces": [
    "apps/web",
    "apps/workers",
    "packages/shared"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev -w apps/web\" \"npm run dev -w apps/workers\"",
    "build": "npm run build --workspaces",
    "start": "npm run start -w apps/web",
    "start:prod": "concurrently \"npm run start -w apps/web\" \"npm run start -w apps/workers\"",
    "lint": "eslint . --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report"
  },
  "devDependencies": {
    "sharp": "^0.33.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/node": "^20.19.24",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "eslint-config-next": "15.0.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-agent": "^3.975.0",
    "@aws-sdk/client-bedrock-agent-runtime": "^3.975.0",
    "@aws-sdk/client-bedrock-runtime": "^3.975.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.975.0",
    "@aws-sdk/client-dynamodb": "^3.975.0",
    "@aws-sdk/client-eventbridge": "^3.975.0",
    "@aws-sdk/client-s3": "^3.975.0",
    "@aws-sdk/client-secrets-manager": "^3.975.0",
    "@aws-sdk/client-ses": "^3.975.0",
    "@aws-sdk/client-textract": "^3.975.0",
    "@aws-sdk/lib-dynamodb": "^3.975.0",
    "@aws-sdk/s3-request-presigner": "^3.975.0",
    "@headlessui/react": "^2.2.9",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-tabs": "^1.0.4",
    "@types/pdfkit": "^0.17.4",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.2.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "docx": "^9.5.1",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "isomorphic-dompurify": "^2.3.0",
    "jose": "^5.2.0",
    "lucide-react": "^0.294.0",
    "mammoth": "^1.11.0",
    "mermaid": "^11.0.0",
    "next": "^15.5.6",
    "next-auth": "^5.0.0-beta.20",
    "openai": "^4.24.0",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.4",
